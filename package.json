{
  "name": "copilot_test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc -p src-react/tsconfig.json --noEmit && tsc -p src-admin/tsconfig.json --noEmit && npm run build:react && npm run build:admin",
    "build:backend": "tsc",
    "build:react": "vite build",
    "build:admin": "vite build --config vite.admin.config.ts",
    "build:production": "npx prisma generate && npx prisma migrate deploy && npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "dev:react": "vite",
    "dev:admin": "vite --config vite.admin.config.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:react\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:react\" \"npm run dev:admin\"",
    "clean": "rm -rf dist public/react public/admin",
    "test": "cross-env NODE_ENV=test jest",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:status": "npx prisma migrate status",
    "db:resolve": "npx prisma migrate resolve --applied",
    "db:studio": "npx prisma studio",
    "env:dev": "copy .env.development .env",
    "env:prod": "copy .env.production .env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@react-oauth/google": "^0.12.2",
    "@sentry/node": "^10.31.0",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/google.maps": "^3.58.1",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^1.41.3",
